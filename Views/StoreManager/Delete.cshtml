@model MvcApplication3.Models.Store
@{
    ViewBag.Title = "Delete";
}
@using MvcApplication3.Models
<h2>Delete Confirmation</h2>
<html>
<body style="background-image:url('/Content/images/y.jpg')">

    <p>Are you sure you want to delete this album? </p>

    @using (Html.BeginForm("DeleteAlbum", "StoreManager", FormMethod.Post))
    {
        @Html.HiddenFor(model => model.Id)
        
        @Html.HiddenFor(model => model.Artist)
        @Html.HiddenFor(model => model.Title)
        @Html.HiddenFor(model => model.Genre)
        
        <p>         <input type="submit" value="Delete" />     </p>
        <p>         @Html.ActionLink("Back to List", "Main")     </p>

    }

</body>

</html>